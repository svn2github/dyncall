download_zip()
{
  wget http://dyncall.org/r0.3/dyncall-0.3.zip
  unzip dyncall-0.3
  mv dyncall-0.3 src/dyncall
  rm -Rf dyncall-0.3.zip
}

checkout_src()
{
  svn co http://dyncall.org/svn/dyncall/branches/autodetect/dyncall src/dyncall
  svn co https://134.76.186.6/svn/repos/dadler/dyncall_callbacks src/dyncall_callbacks
  svn co https://134.76.186.6/svn/repos/dadler/dyncall_callbacks src/dyncall_callbacks
}

# download_zip
checkout_src
