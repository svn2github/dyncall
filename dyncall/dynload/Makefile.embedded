MAKEFILE  = Makefile.embedded
MAKE_CMD  = ${MAKE} -f Makefile.embedded
TARGET    = libdynload_s.a
OBJS      = dynload.o dynload_syms.o
CPPFLAGS += -I../dyncall
all: ${TARGET}
libdynload_s.a: ${OBJS}
	${AR} cr $@ ${OBJS}
clean:
	rm -f ${OBJS} ${TARGET}
.PHONY: all clean
sun-64bit:
	CFLAGS="${CFLAGS} -m64" ASFLAGS="${ASFLAGS} -m64" ${MAKE_CMD} all
sun-gcc:
	CC=gcc CFLAGS="${CFLAGS} -fPIC" ${MAKE_CMD} all
