TOP=../..

.include "$(TOP)/build/bsdmake/prolog.bsdmake"


MODS	=	case main
APP	=	suite2

CPPFLAGS	=
LDFLAGS		=
LIBS	=	"$(TOP)/dyncall/libdyncall_s.a"

AUTOS	=	config.h case.h


.include "$(TOP)/build/bsdmake/epilog.bsdmake"


.PHONY: config 
config: $(AUTOS)
$(AUTOS): mkcase.py .SILENT
	echo Generating test cases...
	python mkcase.py >case.h 2>config.h

