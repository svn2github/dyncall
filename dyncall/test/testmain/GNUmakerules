LINK_LIBS	+= pspdebug pspdisplay pspge pspctrl \
pspsdk c pspnet pspnet_inet pspnet_apctl pspnet_resolver \
psputility pspuser pspkernel
endif

include $(TOP)/build/gmake/epilog.gmake

