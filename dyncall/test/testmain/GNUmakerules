include $(TOP)/build/gmake/prolog.gmake

LINK_PATHS	+= $(TOP)/test/testmain/$(BUILD_DIR) 
LINK_LIBS	+= testmain_s 

ifdef BUILD_OS_psp
PSPSDK		=  $(shell psp-config --pspsdk-path)
CPPFLAGS	=  -I$(PSPSDK)/include
