APP = test_thunk
OBJS = test_thunk.o

TOP = ../..
CFLAGS = -I${TOP}/dyncallback -I${TOP}/dyncall
LDFLAGS = -L${TOP}/dyncallback -ldyncallback_s
LINK = ${CC}

all: ${APP}

.PHONY: all clean install

RM      ?= rm -f
AR      ?= ar
MKDIR	?= mkdir
INSTALL	?= install -p

${APP}: ${OBJS} 
	${LINK} ${OBJS} ${LDFLAGS} -o ${APP}

clean:
	${RM} ${APP} ${OBJS}

